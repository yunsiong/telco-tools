{
  "name": "itracer-agent",
  "version": "1.0.0",
  "description": "Agent used by telco-itrace",
  "private": true,
  "main": "agent.ts",
  "type": "module",
  "scripts": {
    "build": "telco-compile agent.ts -c -o ../../telco_tools/itracer_agent.js",
    "watch": "telco-compile agent.ts -w -o ../../telco_tools/itracer_agent.js"
  },
  "devDependencies": {
    "@types/telco-gum": "^18.4.0",
    "@types/node": "^20.4.2",
    "telco-compile": "^16.3.0"
  },
  "dependencies": {
    "telco-itrace": "^2.0.2"
  }
}
